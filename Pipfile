[[source]]
name = "pypi"
url = "https://pypi.org/simple"
verify_ssl = true

[scripts]
build = "python setup.py sdist bdist_wheel"
test = "python -m twine upload --repository testpypi dist/*"
upload = "python -m twine upload dist/*"

[dev-packages]
autopep8 = "*"
pylint = "*"
twine = "*"

[requires]
python_version = "3.9"
